{
    "name": "myunet/queue",
    "description": "A queue service for ThinkPHP framework",
    "type": "library",
    "keywords": [
        "queue",
        "thinkphp",
        "service"
    ],
    "homepage": "https://example.com",
    "license": "MIT",
    "authors": [
        {
            "name": "myunet",
            "email": "myunet@163.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "topthink/framework": "^6.0|^7.0|^8.0",
        "topthink/think-orm": "^2.0|^3.0|^4.0",
        "symfony/process": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "myunet\\": "src/"
        },
        "files": [
            "src/common.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "myunet\\Library"
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
